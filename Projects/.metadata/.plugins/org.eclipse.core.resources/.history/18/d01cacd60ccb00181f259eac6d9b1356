/**
 * Node Class
 * @author Mikael Hinton
 *
 */
public class Node {
	
	private Object item;
	private Node next;
	
	Node(Object newItem)
	{
		item = newItem;
		next = null;
	}
	
	Node(Object newItem, Node nextNode)
	{
		item = newItem;
		next = nextNode;
	}
	
	public Object getNode()
	{
		return item;
	}
	
	public void display()
	{
		for(Node cur =; cur != null; cur = cur.next)
		{
			System.out.println(cur.item);
		}
	}
}//EOF
