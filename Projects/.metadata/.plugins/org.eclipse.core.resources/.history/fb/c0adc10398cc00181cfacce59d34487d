/**
 * Node Class
 * @author Mikael Hinton
 *
 */
public class Node {
	
	Object item;
	Node next;
	
	/*
	 * Constructor
	 */
	protected Node(Object newItem)
	{
		item = newItem;
		next = null;
	}
	
	/*
	 * Constructor
	 */
	protected Node(Object newItem, Node nextNode)
	{
		item = newItem;
		next = nextNode;
	}
}//EOF
