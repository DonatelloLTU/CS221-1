/**
 * Node Class
 * @author Mikael Hinton
 *
 */
public class Node {
	
	Object item;
	Node next;
	
	Node(Object newItem)
	{
		item = newItem;
		next = null;
	}
	
	Node(Object newItem, Node nextNode)
	{
		item = newItem;
		next = nextNode;
	}
	public void display()
	{
		
	}
	for(Node cur = head; cur != null; cur = cur.next)
	{
		System.out.println(cur.item);
	}
}//EOF
